class Solution {
    public String restoreString(String s, int[] indices) {
        // s[0]
        char dup[] = new char[indices.length];
        for(int i = 0 ; i<indices.length; i++){
            dup[indices[i]] = s.charAt(i);
        }
        return new String(dup); // char array convert into string
    }
}
